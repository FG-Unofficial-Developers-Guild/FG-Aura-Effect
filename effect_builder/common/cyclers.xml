<root version="3.0">
    <template name="cycler_eb_aura_faction">
        <cycler_eb_valueholder name="aura_faction">
            <parameters mergerule="replace">
                <defaultlabel>all</defaultlabel>
                <defaultvalue>all</defaultvalue>
                <labels>friend|foe</labels>
                <values>friend|foe</values>
            </parameters>
        </cycler_eb_valueholder>
    </template>

    <template name="cycler_eb_aura_faction_conditional">
        <cycler_eb_valueholder name="aura_faction_conditional">
            <parameters mergerule="replace">
                <defaultlabel>notself</defaultlabel>
                <defaultvalue>notself</defaultvalue>
                <labels>friend|not friend|foe|not foe|neutral|not neutral</labels>
                <values>friend|^friend|foe|^foe|neutral|^neutral</values>
            </parameters>
        </cycler_eb_valueholder>
    </template>
</root>