<root version="3.0">
    <template name="cycler_eb_aura_faction">
        <cycler_eb_valueholder name="aura_faction">
            <parameters mergerule="replace">
                <defaultlabel>all</defaultlabel>
                <defaultvalue>all</defaultvalue>
                <labels>ally|not ally|enemy|not enemy|friend|not friend|foe|not foe|none|not none|self||not self|neutral|not neutral|</labels>
                <values>ally|!ally|enemy|!enemy|friend|!friend|foe|!foe|none|!none|self|!self|neutral|!neutral|</values>
            </parameters>
        </cycler_eb_valueholder>
    </template>

    <template name="cycler_eb_aura_faction_conditional">
        <cycler_eb_valueholder name="aura_faction_conditional">
            <parameters mergerule="replace">
                <defaultlabel>not self</defaultlabel>
                <defaultvalue>!self</defaultvalue>
                <labels>self|ally|not ally|enemy|not enemy|friend|not friend|foe|not foe|none|not none|neutral|not neutral|</labels>
                <values>self|ally|!ally|enemy|!enemy|friend|!friend|foe|!foe|none|!none|neutral|!neutral|</values>
            </parameters>
        </cycler_eb_valueholder>
    </template>
</root>